{
  "manifest_version": 2,
  "name": "Colorful To-Do List",
  "version": "1.0",
  "description": "A colorful to-do list Chrome extension.",
  "permissions": ["storage"],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Colorful To-Do List"
  },
  "icons": {
    "48": "icon.png"
  }
}